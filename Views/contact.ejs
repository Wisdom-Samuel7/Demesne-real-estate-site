        font-style: normal;
        font-size: 18px;
        position: relative;
    }

    nav a:hover {
        color: powderblue;
    }

    nav a:hover::before {
        position: absolute;
        content: '';
        height: 3px;
        width: 100%;
        background: khaki;
        bottom: -5px;
        animation: hoverEffect .2s forwards ease-in;
    }


    .links {
