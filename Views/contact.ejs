        margin-top: 2em;
        grid-template-columns: 1fr 1fr;
        gap: 1em;
        z-index: 4000;
    }

    @media screen and (min-width:760px) {
        .first {
            width: 100vw;
            height: 100vh;
            display: grid;
            grid-template-columns: 2fr 1fr;
        }

        .b {
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            padding: 6em 1em;
        }

        .b img {
            width: 100%;
            height: 360px;
            object-fit: cover;
            border-radius: .5em;
            rotate: 3deg;
        }

        .sec {
            width: 100vw;
            height: 50vh;
            display: grid;
            grid-template-rows: 1fr 1fr;
            color: #fff;
            padding: 0em 0em;
        }

        .media {
            flex-direction: row;
            justify-content: center;
            align-items: center;
            gap: 8em;
        }

        .sec h1 {
