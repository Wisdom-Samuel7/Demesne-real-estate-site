    }

    .b {
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
    }

    .b img {
        width: 60%;
        height: 90%;
        object-fit: cover;
        border-radius: .5em;
    }

    .sec {
        width: 100vw;
        height: 100vh;
        display: grid;
        grid-template-rows: 1fr 1fr;
        color: #fff;
        padding: 7em 0em;
    }

    .sec h1 {
        font-family: "Poppins", sans-serif;
        text-align: center;
        font-size: 22px;
    }

    .media {
        width: 100vw;
        height: inherit;
        display: flex;
        flex-direction: column;
        justify-content: start;
        align-items: center;
        padding: 4em .2em;
        gap: 2em;
    }

    .items {
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        gap: .1em;
        font-family: "Poppins", sans-serif;
    }

    .items h2 {
        color: orange;
        font-size: 25px;

    }

    .items p {
        color: darkgray;
    }


    footer{
        width: 100vw;
        height: 50vh;

    }

    footer {
        width: 100vw;
        height: 30vh;
        background: #fff;
        display: grid;
        grid-template-columns: 1fr 1fr 1fr;
        place-items: center;
        font-weight: bolder;
     
    }

    .foot-a { 
        display: flex;
        flex-direction: column;
        flex-wrap: wrap;
        gap: 1em;
        z-index: 4000;
    }

    .foot-a a {
        color: #000;
        font-weight: bold;
        font-style: normal;
